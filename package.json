{
  "name": "runebot",
  "version": "1.1.0",
  "description": "A bot with useful commands for Old School Runescape",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "dev": "cross-env NODE_ENV=development node --trace-warnings app.js",
    "generate-docs": "jsdoc -c .jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Flawedspirit/RuneBot"
  },
  "keywords": [
    "2007scape",
    "runescape",
    "oldschool",
    "rs2",
    "osrs"
  ],
  "author": "Flawedspirit",
  "license": "MIT",
  "dependencies": {
    "ascii-table": "0.0.9",
    "chalk": "^4.1.0",
    "discord.js": "^12.3.1",
    "osrs-exchange": "^1.0.2",
    "require-reload": "^0.2.2",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "jsdoc": "^3.6.6",
    "jsdoc-template": "github:braintree/jsdoc-template",
    "travis": "^0.1.1"
  }
}
